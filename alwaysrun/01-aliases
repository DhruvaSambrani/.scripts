alias cd~="cd ~"
alias cd..="cd .."
alias ls=lsd
alias sl=ls
alias q=exit
alias py=python3
alias python=python3
alias pip=pip3
alias julia="julia -q --project "
alias pm=pathmover
alias pprb="~/.paperbuilder/src/PaperBuilder.py"
alias calcurse="calcurse -C ~/.calcurse -D ~/.calcurse"
alias cal=calcurse
alias clear="\clear && . ~/.scripts/daily-quote"
alias celar=clear
alias celary=clear
alias vim=nvim
alias vi=nvim
alias ll="ls -A --blocks=size,date,name --date \"+%H:%M %d-%b-%Y\""
alias \?=man
alias \?\?=which
alias qu="source ~/.scripts/daily-quote"
alias feh="feh -x "
